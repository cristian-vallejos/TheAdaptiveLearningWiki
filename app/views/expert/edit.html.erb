  <body>
    <section id="ext_menu-3">
        <nav class="navbar navbar-dropdown navbar-fixed-top">
            <div class="container">
                <div class="mbr-table">
                    <div class="mbr-table-cell">
                        <div class="navbar-brand">
                            <a href="<%= root_path %>" class="navbar-logo"><%= image_tag("talw-172x128.png", :alt => "rss feed") %>
                            </a>
                            <a class="navbar-caption" href="<%= root_path %>">THE ADAPTIVE LEARNING WIKI</a>
                        </div>
                    </div>
                    <div class="mbr-table-cell">
                        <ul class="nav-dropdown collapse pull-xs-right nav navbar-nav navbar-toggleable-sm" id="exCollapsingNavbar" style="height: 100% !important;"><li class="nav-item nav-btn" style="color: white;">Sesión iniciada como <a class="nav-link" href="<%= root_path %>">Cristian Vallejos Pérez</a></li></ul>

                        <ul class="nav-dropdown collapse pull-xs-right nav navbar-nav navbar-toggleable-sm" id="exCollapsingNavbar" style="    margin-right: 17.5%; height: 100% !important;">
                            <li class="nav-item <%= action_name == 'home' ? 'active' : '' %>">
                                <a class="nav-link link" href="<%= experto_path %>">BUSCAR CONTENIDO</a>
                            </li>
                            <li class="nav-item <%= action_name == 'edit' ? 'active' : '' %>">
                                <a class="nav-link link" href="<%= experto_validar_contenido_path %>">VALIDAR CONTENIDO</a>
                            </li>
                        </ul>
                    </div>
                </div>
            </div>
        </nav>
    </section>

    <section class="mbr-section mbr-after-navbar" id="form2-2" style="background-color: rgb(41, 105, 176);padding-top: 150px; padding-bottom: 100%;">
        <div class="mbr-section mbr-section-nopadding">
            <div class="container">
                <div class="row">
                    <div class="col-xs-12 col-lg-12 col-lg-offset-1" data-form-type="formoid" style="margin-left: 6%">
                        <div class="promos">
                        <%
                            contador = 1
                            @archivos.each do |i|
                                if contador%4 == 0 %>
                                    <div class="promo " style="margin-bottom: 10%;" onmouseover="this.onmouseover = null;">
                                      <div class="deal">
                                        <span><p><%= i.nombre %></p></span>
                                      </div>
                                      <span class="price"><a href="<%= i.referencia %>" target="_blank"><img height="40%" width="40%" src="<% if i.tipo == 0 %> http://www.freeiconspng.com/uploads/pdf-icons-free-icons-in-file-icons-18.png <% elsif i.tipo == 1 %> https://thediscourseunit.files.wordpress.com/2016/05/doc1.png <% elsif i.tipo == 2 %> http://www.freeiconspng.com/uploads/ppt-icons-free-icons-in-file-icons-vs-2--icon-search-engine--6.png <% end %>"></a></span>
                                      <button type="button" class="button success"><a href="<%= experto_aprove_path+'?id='+i.id.to_s %>">Aprobar</a></button>
                                      <button type="button" class="button danger"><a href="<%= experto_denie_path+'?id='+i.id.to_s %>">Denegar</a></button>
                                    </div>
                        <%      else %>
                                    <div class="promo " style="margin-bottom: 10%" onmouseover="this.onmouseover = null;">
                                      <div class="deal">
                                        <span><p><%= i.nombre %></p></span>
                                      </div>
                                      <span class="price"><a href="<%= i.referencia %>" target="_blank"><img height="40%" width="40%" src="<% if i.tipo == 0 %> http://www.freeiconspng.com/uploads/pdf-icons-free-icons-in-file-icons-18.png <% elsif i.tipo == 1 %> https://thediscourseunit.files.wordpress.com/2016/05/doc1.png <% elsif i.tipo == 2 %> http://www.freeiconspng.com/uploads/ppt-icons-free-icons-in-file-icons-vs-2--icon-search-engine--6.png <% end %>"></a></span>
                                      <button type="button" class="button success"><a href="<%= experto_aprove_path+'?id='+i.id.to_s %>">Aprobar</a></button>
                                      <button type="button" class="button danger"><a href="<%= experto_denie_path+'?id='+i.id.to_s %>">Denegar</a></button>
                                    </div>
                        <%      end
                            contador = contador + 1
                            end %>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </section>
  </body>
