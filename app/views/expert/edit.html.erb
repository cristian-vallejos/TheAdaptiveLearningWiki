    <style>
        .active {background-color: grey;}

        @import url(http://fonts.googleapis.com/css?family=Open+Sans:400,600);
        html {
          background: rgb(246, 248, 248);
        }
        body {
          font-family: 'Open Sans', sans-serif;
          font-size: 14px;
          font-weight: 400;  
        }
        .promos {
          width: 100%;
          margin: 0 auto;
          margin-top: 50px;
        }
        .promo {
          width: 250px;
          background: #0F1012; 
          color: #f9f9f9;
          float: left;
          margin-right: 0.25%;
          border-radius: 5%;
        }
        .deal {
          padding: 10px 0 0 0;
        }
        .deal span {
          display: block;
          text-align: center;
        }
        .deal span:first-of-type {
          font-size: 23px;  
        }
        .deal span:last-of-type {
          font-size: 13px;
        }
        .promo .price {
          display: block;
          width: 250px;  
          background: #292b2e;
          margin: 15px 0 10px 0;
          text-align: center;
          font-size: 23px;
          padding: 17px 0 17px 0;
        }
        .ul {
          display: block;
          margin: 20px 0 10px 0;
          padding: 0;
          list-style-type: none;
          text-align: center;
          color: #999999;
        }
        .li {
          display: block;
          margin: 10px 0 0 0;
        }
        .button {
          border: none;
          border-radius: 40px;
          background: #292b2e;
          color: #f9f9f9;
          padding: 10px;
          margin: 10px 0 20px 60px;
        }
        .success {
            margin-right: 2.5%;
        }
        .success, .danger {
            margin-left: 5%;
            width: 40%;
            border-radius: 7.5%;
        }
        .success:hover {
            background: #31cb55;
        }
        .danger:hover {
            background: #c92727;
        }
        .scale {
          transform: scale(1.2);
          box-shadow: 0 0 4px 1px rgba(20, 20, 20, 0.8);
        }
        .scale button {
          background: #64AAA4;
        }
        .scale .price {
          color: #64AAA4;
        }
        p {
          margin-left: 5%;
          white-space: nowrap;
          overflow: hidden;
          text-overflow: ellipsis;
          max-width: 200px;
        }
    </style>

    <script>
        function Scale(x) {
            x.className += "scale";
        }
    </script>
